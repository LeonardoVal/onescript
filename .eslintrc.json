{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
      "camelcase": "off",
      "no-nested-ternary": "off",
      "no-param-reassign": "off",
      "no-unused-vars": "off",
      "object-curly-newline": "off"
    }
}
